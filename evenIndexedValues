class Main {
    public static void main(String[] args) {
        String st="anupriya";
        StringBuffer sb=new StringBuffer(st);
        int count=0;
        for(int i=0;i<sb.length();i++){
            if(i%2==0){
                count++;
            }
        }
        System.out.println(count);
    }
}
