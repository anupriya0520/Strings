class Main {
    public static void main(String[] args) {
        String st="16382539";
        StringBuffer sb=new StringBuffer(st);
        int evencount=0;
        int oddcount=0;
        for(int i=0;i<sb.length();i++){
            if(Character.getNumericValue(sb.charAt(i))%2==0){
                evencount++;
            }else{
                oddcount++;
            }
        }
        System.out.println(evencount);
        System.out.println(oddcount);
    }
}
